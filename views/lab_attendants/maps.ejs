        <script>
          // Initialize and add the map
          var map, marker;
          // The location of Uluru,
          var uluru = {lat: 7.301657, lng: 5.135383};
          function initMap() {
            // The map, centered at Uluru
            map = new google.maps.Map(
                document.getElementById('map'), {zoom: 19, center: uluru,gestureHandling: 'greedy'});
            // The marker, positioned at Uluru
            // map.setOptions({ draggableCursor : "url(http://s3.amazonaws.com/besport.com_images/status-pin.png), ponter" })
            marker = new google.maps.Marker({position: uluru, map: map});
          }

          setInterval(() => {
              uluru.lat += 0.0001
              uluru.lng += 0.0001
              map.panTo(uluru);
              marker = new google.maps.Marker({position: uluru, map: map});
          }, 20000);

          /*
